{
	"name": "cursorless-sidecar",
	"displayName": "Cursorless Sidecar",
	"description": "Adds commands to VSCode to allow it to be used as a sidecar that is synchronized to a different editor",
	"version": "0.0.284",
	"publisher": "phillco",
	"license": "MIT",
	"engines": {
		"vscode": "^1.64.0"
	},
	"repository": "https://github.com/phillco/cursorless-sidecar.git",
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onStartupFinished"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "sidecar.openPath",
				"title": "Sidecar: Open Path"
			},
			{
				"command": "sidecar.setCursor",
				"title": "Sidecar: Set Cursors"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.2.0",
		"@types/mocha": "^9.1.0",
		"@types/node": "14.x",
		"@types/vscode": "^1.64.0",
		"@typescript-eslint/eslint-plugin": "^5.12.1",
		"@typescript-eslint/parser": "^5.12.1",
		"@vscode/test-electron": "^2.1.2",
		"eslint": "8.22.0",
		"glob": "^7.2.0",
		"lodash": "^4.17.21",
		"mocha": "^9.2.1",
		"typescript": "^4.5.5"
	},
	"dependencies": {
		"@types/lodash": "^4.14.168",
		"lowdash": "^1.2.0"
	}
}
